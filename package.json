{
  "name": "upe-discord-bot",
  "version": "1.0.0",
  "description": "UPE induction Discord bot",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node .",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "postbuild": "copyfiles -u 1 src/assets/**/* dist",
    "dev": "ts-node src/index.ts",
    "sync": "ts-node src/index.ts --sync",
    "env-example": "ts-node scripts/update-env-example.ts",
    "prepare": "husky",
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinlin24/upe-discord-bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vinlin24/upe-discord-bot/issues"
  },
  "homepage": "https://github.com/vinlin24/upe-discord-bot",
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "cheerio": "^1.0.0-rc.12",
    "csv-string": "^4.1.1",
    "discord.js": "^14.8.0",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "googleapis": "^144.0.0",
    "image-downloader": "^4.3.0",
    "lodash": "^4.17.21",
    "luxon": "^3.6.1",
    "mongoose": "^7.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/lodash": "^4.17.16",
    "@types/luxon": "^3.6.2",
    "@types/mongodb": "^4.0.6",
    "@types/node": "^18.15.10",
    "copyfiles": "^2.4.1",
    "eslint": "^9.25.1",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^9.1.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.31.0"
  }
}
